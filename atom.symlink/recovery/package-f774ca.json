{
  "name": "@dooer/fe-ui-kit",
  "version": "7.1.0",
  "description": "Frontend UI Kit for Dooer projects",
  "main": "build/index.js",
  "scripts": {
    "clean": "rm -rf ./dist/ && rm -rf ./build/",
    "build": "npm-run-all clean --parallel build:*",
    "build:js": "babel -d build src",
    "build:css": "node-sass src/styles --output build --output-style compressed",
    "watch": "npm-run-all --parallel watch:*",
    "watch:js": "babel --watch -d build src",
    "watch:css": "node-sass --watch src/styles --output build --output-style compressed",
    "prepublish": "npm run build",
    "test": "standard"
  },
  "files": [
    "build",
    "dist"
  ],
  "repository": "Dooer/fe-ui-kit",
  "license": "UNLICENSED",
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "@dooer/babel-preset-dooer"
    ],
    "plugins": [
      [
        "react-intl",
        {
          "messagesDir": "./dist/messages/"
        }
      ]
    ]
  },
  "devDependencies": {
    "@dooer/babel-preset-dooer": "^1.0.0",
    "@dooer/cz": "^0.5.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-intl": "^2.3.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "@dooer/fe-icon-kit": "^1.4.0",
    "chroma-js": "^1.3.3",
    "classnames": "^2.2.3",
    "closest": "0.0.1",
    "draft-js": "^0.10.1",
    "draftjs-md-converter": "^0.1.5",
    "keycode": "^2.1.9",
    "object.omit": "^2.0.1",
    "prop-types": "^15.5.10",
    "radium": "^0.19.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-intl": "^2.3.0",
    "recompose": "^0.24.0",
    "warning": "^3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@dooer/cz"
    }
  }
}
