{
  "files.autoSave": "onFocusChange",
  "editor.formatOnSave": true,
  "editor.fontFamily": "'FiraCode Nerd Font', 'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.cursorStyle": "line",
  "editor.wordWrap": "on",
  "eslint.validate": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact"
  ],
  "auto-correct.dictionary": [
    {
      "languages": ["*"],
      "words": {
        "attachemnt": "attachment",
        "hte": "the",
        "mispell": "misspell",
        "mispelled": "misspelled",
        "{despa,sepe}rat{e,es,ed,ing,ely,ion,ions,or}": "{despe,sepa}rat{}"
      },
      "useLargeList": false
    },
    {
      "languages": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
      ],
      "words": {
        "cosnt": "const",
        "functoin": "function"
      }
    }
  ],
  "editor.fontLigatures": true,
  "files.exclude": {
    "**/.git": true,
    "src/**/*.bs.js": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/build": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "editor.scrollBeyondLastLine": false,
  "workbench.editor.showIcons": true,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "emphasis",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "strong",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": "entity.other.attribute-name",
        "settings": {
          // "fontStyle": "italic"
        }
      },
      {
        "scope": "markup.underline",
        "settings": {
          "fontStyle": "underline"
        }
      },
      {
        "scope": "markup.bold",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": "markup.heading",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": "markup.italic",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "storage.type",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "storage.modifier",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "String interpolation",
        "scope": [
          "punctuation.definition.template-expression.begin",
          "punctuation.definition.template-expression.end",
          "punctuation.section.embedded"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "keyword.operator.object",
          "keyword.operator.new",
          "keyword.operator.expression",
          "keyword.operator.cast",
          "keyword.operator.sizeof",
          "keyword.operator.logical.python"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "keyword.control - variable.interpolation",
          "keyword.control.open",
          "keyword.other"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "this.self",
        "scope": "variable.language",
        "settings": {
          "fontStyle": "italic",
          "foreground": "#ff5874"
        }
      },
      {
        "name": "@Decorator",
        "scope": ["meta.decorator punctuation.decorator"],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["punctuation.definition.comment", "comment"],
        "settings": {
          // "foreground": "#ff5874",
          "fontStyle": "italic"
        }
      }
    ]
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#000000",
    "dropdown.background": "#000000",
    "dropdown.foreground": "#202020",
    "scrollbar.shadow": "#00000000",
    "input.background": "#1f1f1f",
    "panel.background": "#000000",
    "peekViewEditor.background": "#000000",
    "scrollbarSlider.activeBackground": "#000000",
    "scrollbarSlider.hoverBackground": "#363636",
    "scrollbarSlider.background": "#303030",
    "sideBar.background": "#0F0F0F",
    "editorGroup.emptyBackground": "#000000",
    "editor.background": "#000000",
    "editor.selectionBackground": "#5e1c7a",
    "editor.selectionForeground": "#ffffff",
    "editor.inactiveSelectionBackground": "#512261",
    "editor.selectionHighlightBackground": "#4c3653",
    "tab.activeBackground": "#0f0f0f",
    "tab.activeForeground": "#A5A5A5",
    "tab.inactiveForeground": "#6D6D6D",
    "tab.inactiveBackground": "#000000",
    "editorGroupHeader.tabsBackground": "#000000",
    "editorBracketMatch.border": "#1C617A",
    "editorBracketMatch.background": "#09384B",
    "editorGutter.background": "#000000",
    "editorWarning.foreground": "#ffc400",
    "editorError.foreground": "#d44343",
    "editorInfo.foreground": "#2eebcb",
    "editorWidget.border": "#1C617A",
    "terminal.foreground": "#dcdfe4",
    "terminal.background": "#0f0f11",
    "terminal.ansiBlue": "#69dbf0",
    "terminal.ansiBrightBlue": "#aae2ec",
    "terminal.ansiGreen": "#83f197",
    "terminal.ansiBrightGreen": "#c3f0cb",
    "terminal.ansiCyan": "#83f1e2",
    "terminal.ansiBrightCyan": "#bbf7ef",
    "terminal.ansiMagenta": "#f183e8",
    "terminal.ansiBrightMagenta": "#f0b9eb",
    "terminal.ansiBlack": "#6b6b6b",
    "terminal.ansiBrightBlack": "#9e9e9e",
    "terminal.ansiWhite": "#ebebeb",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.ansiRed": "#fa7f7f",
    "terminal.ansiBrightRed": "#f5cdcd",
    "terminal.ansiYellow": "#f3f186",
    "terminal.ansiBrightYellow": "#f3f2b8"
  },
  "editor.minimap.enabled": true,
  "editor.minimap.showSlider": "always",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": false,
  "terminal.external.osxExec": "Warp.app",
  "editor.renderLineHighlight": "all",
  "editor.codeLens": true,
  "workbench.startupEditor": "newUntitledFile",
  "editor.selectionHighlight": false,
  "editor.snippetSuggestions": "top",
  "workbench.colorTheme": "Visual Studio Dark",
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false
  },
  "editor.tabSize": 2,
  "editor.lightbulb.enabled": false,
  "zenMode.centerLayout": true,
  "sync.gist": "1e679e9dce3adba34d884e39a7cfee5a",
  "sync.autoDownload": false,
  "sync.autoUpload": false,
  "sync.forceDownload": false,
  "sync.quietSync": false,
  "sync.askGistName": false,
  "sync.removeExtensions": true,
  "sync.syncExtensions": true,
  "git.ignoreMissingGitWarning": true,
  "git.path": "/usr/local/bin/git",
  "[reason]": {
    "editor.formatOnSave": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.enabled": false,
  "gitlens.views.fileHistory.enabled": false,
  "gitlens.views.search.enabled": false,
  "gitlens.views.compare.enabled": false,
  "javascript.format.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "explorer.confirmDragAndDrop": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "*.symlink": "shellscript"
  },
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "editor.inlineSuggest.enabled": true,
  "workbench.activityBar.visible": false,
  "workbench.statusBar.visible": false,
  "extensions.ignoreRecommendations": true,
  "npm.keybindingsChangedWarningShown": true,
  "gitlens.statusBar.enabled": false,
  "window.commandCenter": false,
  "terminal.integrated.windowsEnableConpty": false,
  "terminal.integrated.defaultProfile.osx": "zsh"
}
