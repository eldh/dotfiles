{
  "files.autoSave": "onFocusChange",
  "editor.fontFamily": "'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.cursorStyle": "line",
  "editor.wordWrap": "on",
  "editor.fontLigatures": true,
  "workbench.statusBar.visible": false,
  "workbench.activityBar.visible": false,
  "files.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/build": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "window.zoomLevel": 0,
  "editor.scrollBeyondLastLine": false,
  "workbench.editor.showIcons": true,
  "editor.tokenColorCustomizations": {},
  "workbench.colorCustomizations": {
    "activityBar.background": "#000000",
    "dropdown.background": "#000000",
    "dropdown.foreground": "#202020",
    "scrollbar.shadow": "#00000000",
    "input.background": "#1f1f1f",
    "panel.background": "#000000",
    "peekViewEditor.background": "#000000",
    "scrollbarSlider.activeBackground": "#000000",
    "scrollbarSlider.hoverBackground": "#363636",
    "scrollbarSlider.background": "#303030",
    "sideBar.background": "#0F0F0F",
    "editorGroup.background": "#000000",
    "editor.background": "#000000",
    "editor.selectionBackground": "#5e1c7a",
    "editor.selectionForeground": "#ffffff",
    "editor.inactiveSelectionBackground": "#512261",
    "editor.selectionHighlightBackground": "#4c3653",
    "tab.activeBackground": "#0f0f0f",
    "tab.activeForeground": "#A5A5A5",
    "tab.inactiveForeground": "#6D6D6D",
    "tab.inactiveBackground": "#000000",
    "editorGroupHeader.tabsBackground": "#000000",
    "editorBracketMatch.border": "#1C617A",
    "editorBracketMatch.background": "#09384B",
    "editorGutter.background": "#000000",
    "editorWarning.foreground": "#ffc400",
    "editorError.foreground": "#d44343",
    "editorInfo.foreground": "#2eebcb"
  },
  "editor.minimap.enabled": true,
  "editor.minimap.showSlider": "always",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": false,
  "prettier.eslintIntegration": true,
  "prettier.printWidth": 100,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "prettier.semi": false,
  "terminal.external.osxExec": "iTerm.app",
  "editor.renderLineHighlight": "all",
  "editor.codeLens": true,
  "workbench.startupEditor": "newUntitledFile",
  "eslint.autoFixOnSave": true,
  "workbench.colorTheme": "Fedu",
  "editor.selectionHighlight": false,
  "editor.snippetSuggestions": "top"
}